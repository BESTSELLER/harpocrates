  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "request": "attach",
      "name": "Attach by Process ID",
      "processId": "${command:PickJavaProcess}"
    },
    {
      "name": "Launch",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "env": {
        "VAULT_ADDRESS": "https://vault.bestsellerit.com"
      },
      "args": [
        "dev",
        "-f",
        "./secrets-local.yaml",
        "echo \"The value of GithubOrganization is $GithubOrganization\""
      ]
    }
  ]
}
